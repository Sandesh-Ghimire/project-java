<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    
    <servlet>
		<servlet-name>reg</servlet-name>
		<servlet-class>com.resource.register</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>reg</servlet-name>
		<url-pattern>/regF</url-pattern>
	</servlet-mapping>
        
         <servlet>
		<servlet-name>log</servlet-name>
		<servlet-class>com.resource.login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>log</servlet-name>
		<url-pattern>/love</url-pattern>
	</servlet-mapping>
        

        
        <servlet>
		<servlet-name>profile</servlet-name>
		<servlet-class>com.resource.profile</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>profile</servlet-name>
		<url-pattern>/profile</url-pattern>
	</servlet-mapping>
        
         <servlet>
		<servlet-name>PasswordChange</servlet-name>
		<servlet-class>com.resource.PasswordChange</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PasswordChange</servlet-name>
		<url-pattern>/hate</url-pattern>
	</servlet-mapping>
        
 <servlet>
		<servlet-name>delete</servlet-name>
		<servlet-class>com.resource.delete</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>delete</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
        
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
